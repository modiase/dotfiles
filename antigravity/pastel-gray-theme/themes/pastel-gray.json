    "panelTitle.activeForeground": "#e0e0e0",
    "panelTitle.inactiveForeground": "#707070",

    "minimap.selectionHighlight": "#3a3a3a",
    "minimap.findMatchHighlight": "#8fa8c9",

    "editorWidget.background": "#2a2a2a",
    "editorWidget.border": "#3a3a3a",
    "editorSuggestWidget.background": "#2a2a2a",
    "editorSuggestWidget.border": "#3a3a3a",
    "editorSuggestWidget.selectedBackground": "#3a3a3a",
    "editorSuggestWidget.highlightForeground": "#a8d8ea",

    "editorError.foreground": "#d08080",
    "editorWarning.foreground": "#d8d0b8",
    "editorInfo.foreground": "#8fa8c9",
    "editorHint.foreground": "#a8c99a",

    "diffEditor.insertedTextBackground": "#a8c99a20",
    "diffEditor.removedTextBackground": "#d0808020"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#707070",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#d8d0b8"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#a8c99a"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#a8c99a"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control", "keyword.operator.new", "keyword.operator.expression", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c9a8c9"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#a8d8ea"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#8fa8c9"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#f4b6c2"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "meta.type.annotation"],
      "settings": {
        "foreground": "#f4b6c2"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#d08080"
      }
    },
    {
      "name": "Variable - Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#d08080"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#d8d0b8"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#a8d8ea"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#a8d8ea"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "Support",
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#a8d8ea"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#8fa8c9"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#8fa8c9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#f4b6c2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#c9a8c9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#a8d8ea"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#a8c99a"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#d08080",
        "fontStyle": "underline"
      }
    }
  ]
}
