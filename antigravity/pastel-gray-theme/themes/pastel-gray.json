    "tab.inactiveBackground": "#1c1c1c",
    "tab.inactiveForeground": "#707070",
    "tab.border": "#1c1c1c",
    "tab.activeBorder": "#8fa8c9",
    "editorGroupHeader.tabsBackground": "#1c1c1c",

    "statusBar.background": "#2a2a2a",
    "statusBar.foreground": "#e0e0e0",
    "statusBar.debuggingBackground": "#d08080",
    "statusBar.debuggingForeground": "#1c1c1c",
    "statusBar.noFolderBackground": "#2a2a2a",

    "titleBar.activeBackground": "#1c1c1c",
    "titleBar.activeForeground": "#e0e0e0",
    "titleBar.inactiveBackground": "#1c1c1c",
