    "input.background": "#2a2a2a",
    "input.foreground": "#e0e0e0",
    "input.border": "#3a3a3a",
    "input.placeholderForeground": "#707070",
    "inputOption.activeBorder": "#8fa8c9",
    "focusBorder": "#8fa8c9",

    "dropdown.background": "#2a2a2a",
    "dropdown.foreground": "#e0e0e0",
    "dropdown.border": "#3a3a3a",

    "button.background": "#8fa8c9",
    "button.foreground": "#1c1c1c",
    "button.hoverBackground": "#a0b8d9",

    "badge.background": "#8fa8c9",
    "badge.foreground": "#1c1c1c",

    "scrollbar.shadow": "#1c1c1c",
    "scrollbarSlider.background": "#3a3a3a80",
    "scrollbarSlider.hoverBackground": "#70707080",
    "scrollbarSlider.activeBackground": "#909090",

    "terminal.background": "#1c1c1c",
    "terminal.foreground": "#e0e0e0",
    "terminal.ansiBlack": "#1c1c1c",
    "terminal.ansiRed": "#d08080",
    "terminal.ansiGreen": "#a8c99a",
    "terminal.ansiYellow": "#d8d0b8",
    "terminal.ansiBlue": "#8fa8c9",
    "terminal.ansiMagenta": "#c9a8c9",
    "terminal.ansiCyan": "#a8d8ea",
    "terminal.ansiWhite": "#e0e0e0",
    "terminal.ansiBrightBlack": "#3a3a3a",
    "terminal.ansiBrightRed": "#f4b6c2",
    "terminal.ansiBrightGreen": "#b8d9aa",
    "terminal.ansiBrightYellow": "#e0d8c0",
    "terminal.ansiBrightBlue": "#a0b8d9",
    "terminal.ansiBrightMagenta": "#d9b8d9",
    "terminal.ansiBrightCyan": "#b8e8f0",
    "terminal.ansiBrightWhite": "#f0f0f0",

